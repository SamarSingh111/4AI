class Main {
    public static int fibonacci(int n) {
        // Base cases
        if (n <= 1) {
            return n;
        }
        
        // Initialize first two numbers
        int prev = 0;
        int current = 1;
        
        // Calculate fibonacci number iteratively
        for (int i = 2; i <= n; i++) {
            int next = prev + current;
            prev = current;
            current = next;
        }
        
        return current;
    }
}
